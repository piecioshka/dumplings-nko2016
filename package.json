{
    "name": "dumplings-nko2016",
    "version": "0.1.0",
    "description": "Dumplings game for Node Knockout 2016",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "sudo node --harmony index.js",
        "dev:server": "nodemon index.js",
        "dev:client": "webpack --progress -w",
        "clear": "rm -rf node_modules/ npm-debug.log public/dist/ typings/",
        "count": "find app -name '*.js' | xargs wc -l | sort",
        "postinstall": "webpack",
        "typings": "typings install"
    },
    "devDependencies": {
        "nodemon": "^1.11.0"
    },
    "dependencies": {
        "babel-core": "^6.18.2",
        "babel-loader": "^6.2.7",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-es2016": "^6.16.0",
        "babel-preset-stage-0": "^6.16.0",
        "express": "^4.13.3",
        "json-loader": "^0.5.4",
        "phaser": "^2.6.2",
        "script-loader": "^0.7.0",
        "socket.io": "^1.3.7",
        "super-event-emitter": "^4.1.4",
        "typings": "^2.0.0",
        "uuid": "^2.0.3",
        "webpack": "^1.13.3"
    },
    "engines": {
        "node": "6.7.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rumblex/nodeknockout2016-dumplings"
    }
}
